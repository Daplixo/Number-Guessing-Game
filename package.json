{
  "name": "number-guessing-game",
  "version": "1.0.0",
  "description": "A number guessing game",
  "scripts": {
    "scss": "sass --watch scss/main.scss:css/styles.min.css --style compressed --no-source-map",
    "scss:build": "sass scss/main.scss:css/styles.min.css --style compressed --no-source-map",
    "scss:expanded": "sass scss/main.scss:css/styles.css --style expanded --no-source-map",
    "build": "sass scss/main.scss:css/styles.min.css --style compressed --no-source-map && sass scss/main.scss:css/styles.css --style expanded --no-source-map",
    "watch:sass": "sass --watch scss:css --style compressed"
  },
  "devDependencies": {
    "sass": "^1.63.6"
  }
}
